package {	import flash.utils.Timer;	import flash.display.Stage;	import flash.media.SoundChannel;	import flash.events.*;	public class Archer extends Enemy	{		public static const DELAY_RESET:int = 150;		public static const PROJECTILE_SPEED:Number = 23;		public static const MAX_HEALTH:Number = 30;		public var delay:int = 0;		public function Archer(gameTimer:Timer,hand:Hand,dragon:Dragon, posX:int, posY:int)		{			super(gameTimer, hand, dragon, posX, posY, MAX_HEALTH);			range = 350;			startSpeed = .7 + .3 * Math.random();			walkingSpeed = startSpeed;			stoppingRange = range * (.3 * Math.random() + .7);		}		protected override function attack()		{			if (delay == 0)			{				delay = DELAY_RESET;				var thrownAngle:Number = Calculate.findLaunchAngle(realX, dragon.realX, realY, dragon.realY, PROJECTILE_SPEED, Calculate.GRAV, true);								var e:CreateEvent = new CreateEvent(CreateEvent.CREATE_AARROW,realX,realY,thrownAngle,PROJECTILE_SPEED);				dispatchEvent(e);			}			delay--;		}	}}