package {	import flash.utils.Timer;	import flash.display.Stage;	import flash.media.SoundChannel;	import flash.events.Event;	import flash.events.TimerEvent;	public class Bird extends Enemy	{		public static const FLYING_HEIGHT = 60;		public var startHeight:int;		public function Bird(gameTimer:Timer,hand:Hand,dragon:Dragon, posX:int, posY:int)		{			super(gameTimer, hand, dragon, posX, posY, 10);			airSpeed = 4 + Math.random();			startHeight = Math.random() * 30 + FLYING_HEIGHT - 15;			realY = startHeight;		}		public override function onTick(e:TimerEvent):void		{			//while grabbed			if (grabbed)			{				realX = hand.realX;				realY = hand.realY;			}			else			{				realX += airSpeed + 300 / Calculate.doDistance(realX, hand.realX, realY, hand.realY);				if (realY < startHeight)				{					realY += .5 * Math.abs(startHeight - realY);				}				else				{					realY -= .5 * Math.abs(startHeight - realY);				}			}			x = realX;			y = realY;		}	}}