package {	import flash.display.MovieClip;	import flash.utils.Timer;	public class Catapult extends Enemy	{		public static const DELAY_RESET:int = 600;		public static const PROJECTILE_SPEED:Number = 23;		public static const MAX_HEALTH:Number = 60;		public var delay:int = 250;				public function Catapult(gameTimer:Timer,hand:Hand,dragon:Dragon, posX:int, posY:int)		{			super(gameTimer, hand, dragon, posX, posY, MAX_HEALTH);			range = 350;			startSpeed = .4 + .3 * Math.random();			walkingSpeed = startSpeed;			stoppingRange = range * (.1 * Math.random() + .9);		}		protected override function attack()		{			if (delay == 0)			{				delay = DELAY_RESET;				var thrownAngle:Number = Calculate.findLaunchAngle(realX, dragon.realX, realY, dragon.realY, PROJECTILE_SPEED, Calculate.GRAV, true);								var e:CreateEvent = new CreateEvent(CreateEvent.CREATE_ROCK,realX,realY,thrownAngle,PROJECTILE_SPEED);				dispatchEvent(e);			}			delay--;		}	}}