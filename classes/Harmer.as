package {	import flash.utils.Timer;	import flash.display.Stage;	import flash.media.SoundChannel;	public class Harmer extends Enemy	{		public static const DELAY_RESET:int = 22;		public static const MAX_HEALTH:Number = 30;		public var delay:int = 0;		public var meleeDamage:int = 2;		public var crush:Crush = new Crush();		public var attackChannel:SoundChannel;		public function Harmer(gameTimer:Timer,hand:Hand,dragon:Dragon, posX:int, posY:int)		{			super(gameTimer, hand, dragon, posX, posY,  MAX_HEALTH);			range = 60;			startSpeed = 1.8 + .6 * Math.random();			walkingSpeed = startSpeed;			stoppingRange = range * (.2 * Math.random() + .8);		}		protected override function doWalkingHeight()		{			walkingHeight = Calculate.GROUND_LEVEL - 150000 / Math.pow(realX - 400,2);			if (walkingHeight < Calculate.GROUND_LEVEL - 190)			{				walkingHeight = Calculate.GROUND_LEVEL - 190;			}		}		protected override function attack()		{			if (delay == 0)			{				if (parent)				{					delay = DELAY_RESET;					dragon.dragonHealth -=  meleeDamage;					attackChannel = crush.play();				}			}			delay--;		}	}}