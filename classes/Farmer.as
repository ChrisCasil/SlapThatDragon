package {	import flash.utils.Timer;	import flash.display.Stage;	import flash.media.SoundChannel;	public class Farmer extends Enemy	{		public static const DELAY_RESET:int = 600;		public static const PROJECTILE_SPEED:Number = 18;		public static const MAX_HEALTH:Number = 25;		public var delay:int = 0;		public var throw1:Throw1 = new Throw1();		public var throw2:Throw2 = new Throw2();		public var throwChannel:SoundChannel;		public function Farmer(gameTimer:Timer,hand:Hand,dragon:Dragon, posX:int, posY:int)		{			super(gameTimer, hand, dragon, posX, posY,  MAX_HEALTH);			range = 200;			startSpeed = .5 + .3 * Math.random();			walkingSpeed = startSpeed;			stoppingRange = range * (.15 * Math.random() + .85);		}		protected override function attack()		{			if (delay == 0)			{				delay = DELAY_RESET;				var thrownAngle:Number = Calculate.findLaunchAngle(realX, dragon.realX, realY, dragon.realY, PROJECTILE_SPEED, Calculate.GRAV, true);				var e:CreateEvent = new CreateEvent(CreateEvent.CREATE_PITCHFORK,realX,realY,thrownAngle,PROJECTILE_SPEED);				dispatchEvent(e);			}			delay--;		}	}}